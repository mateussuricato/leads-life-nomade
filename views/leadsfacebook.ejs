<!DOCTYPE html>
<html lang="pt-br">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/style.css" />
    <script src="/js/script.js"></script>
    <title>Leads Facebook</title>
  </head>
  <body>
    <main class="Home">
      <section class="Home__container">
        <div class="cards">
          <% listClientes.forEach((clientes) => { %>
          <a href="/detalhes/<%= clientes.id %> ">
            <div class="card">
              <img src="/img/12.gif" alt="" />
              <h2><%= clientes.nome %></h2>
              <p><%= clientes.email %></p>
              <p><%= clientes.whatsapp %></p>
              <a onclick="abrirModal()" href="/delete/<%= clientes.id %>" class="excluir"
                ><button>APAGAR</button></a
              >
            </div>
          </a>
          <% }) %>
        </div>
      </section>
     
    </main>
  </body>
</html>
